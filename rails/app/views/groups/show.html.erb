<div class="page-header">
  <h1><%= @group.name %></h1>
</div>

<ul>
  <% @group.including_users.each do |user| %>
    <li><%= user.name %> ( <%= user.email %> )</li>
  <% end %>
</ul>

<%= link_to 'Back', groups_path, class: "btn btn-default"  %>
<%= link_to 'Edit', edit_group_path(@group), class: "btn btn-info"%>
